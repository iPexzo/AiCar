{
  "name": "car-ai-backend",
  "version": "1.0.0",
  "description": "Backend API for Car AI Assistant with Express, TypeScript, and MongoDB",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:safe": "npx kill-port 8001 && nodemon --exec ts-node src/server.ts",
    "dev:clean": "npx kill-port 8001 && npm run build && pm2 restart car-ai-backend",
    "start:safe": "npx kill-port 8001 && node dist/server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop car-ai-backend",
    "pm2:restart": "pm2 restart car-ai-backend",
    "pm2:delete": "pm2 delete car-ai-backend",
    "pm2:logs": "pm2 logs car-ai-backend",
    "pm2:status": "pm2 status",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:followup": "ts-node test-followup-answers.ts",
    "test:smart": "ts-node test-smart-questions.ts",
    "test:current": "ts-node test-current-behavior.ts",
    "kill-port": "npx kill-port 8001"
  },
  "keywords": [
    "car",
    "ai",
    "express",
    "typescript",
    "mongodb",
    "mongoose"
  ],
  "author": "Car AI Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "openai": "^5.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "concurrently": "^9.2.0",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
